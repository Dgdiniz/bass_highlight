{
  "name": "bass-highlight",
  "displayName": "bass_highlight",
  "description": "Bass assembly extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "snes",
        "aliases": [
          "snes",
          "Snes"
        ],
        "extensions": [
          ".snes.asm"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "bass",
        "aliases": [
          "bass",
          "Bass"
        ],
        "extensions": [
          ".asm",
          ".nes.asm",
          ".md.asm",
          ".megadrive.asm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "snes",
        "scopeName": "source.bass",
        "path": "./syntaxes/snes.asm.tmLanguage.json"
      }
    ]
  }
}